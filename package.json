    "@types/react": "^19.1.4",
    "@types/react-dom": "^19.1.5",
    "astro": "^5.7.13",
    "preact": "^10.26.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "solid-js": "^1.9.7",
    "svelte": "^5.31.1",
